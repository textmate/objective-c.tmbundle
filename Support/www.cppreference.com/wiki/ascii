	</tr>
	<tr class="row22">
		<td class="col0">21</td><td class="col1">25</td><td class="col2">15</td><td class="col3">NAK</td><td class="col4">negative acknowledge</td>
	</tr>
	<tr class="row23">
		<td class="col0">22</td><td class="col1">26</td><td class="col2">16</td><td class="col3">SYN</td><td class="col4">synchronous idle</td>
	</tr>
	<tr class="row24">
		<td class="col0">23</td><td class="col1">27</td><td class="col2">17</td><td class="col3">ETB</td><td class="col4">end of transmission block</td>
	</tr>
	<tr class="row25">
		<td class="col0">24</td><td class="col1">30</td><td class="col2">18</td><td class="col3">CAN</td><td class="col4">cancel</td>
	</tr>
